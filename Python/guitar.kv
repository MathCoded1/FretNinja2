#:kivy 2.0.0

<Neck>:
    canvas:
        Color:
            rgba: .1, .1, 1, .9
        Line:
            width: 2.
            rectangle: (self.x, self.y, self.width, self.height)
    Label:
        center: root.center
<OpenFret>:
    canvas:
        Color:
            rgba: .1, .1, 1, .9
        Line:
            points: self.width, self.height, self.width+100, self.height
